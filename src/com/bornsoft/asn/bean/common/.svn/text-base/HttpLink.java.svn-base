package com.bornsoft.asn.bean.common;

/**
 * Web Link
 * @author Ken Liu Kunyun
 *
 */
public class HttpLink {
	private String text="...";
	private String href="#";
	private String target="_self";
	private int timeSeconds=0;
	
	/**
	 * 
	 * @param text text of link
	 * @param href url of link
	 * @param target target window
	 * @param timeSeconds auto load time in seconds, 0 for no auto loading
	 */
	public HttpLink(String text, String href, String target, int timeSeconds){
		this.text=text;
		this.href=href;
		this.target=target;
		this.timeSeconds=timeSeconds;
	}

	public String getText() {
		return text;
	}
	public void setText(String text) {
		this.text = text;
	}
	public String getHref() {
		return href;
	}
	public void setHref(String href) {
		this.href = href;
	}
	public String getTarget() {
		return target;
	}
	public void setTarget(String target) {
		this.target = target;
	}
	public int getTimeSeconds() {
		return timeSeconds;
	}
	public void setTimeSeconds(int timeSeconds) {
		this.timeSeconds = timeSeconds;
	}
}
