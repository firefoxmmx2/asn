package com.bornsoft.asn.bean.information;

import java.io.Serializable;
import java.util.Date;

public class Info implements Serializable {
	protected Integer infoId;
	protected String title;
	protected String type;
	protected String typeName;
	protected String infoStatus;
	protected String keyWord;
	protected String infoValidity;//信息有效期
	protected String detail;
	protected Date startDate;
	protected Date modifyDate;
	protected Date endDate;
	protected Date delDate;
	protected String ipAddreess;
	protected String area;
	protected Integer censor=3;
	public Info() {
		super();
	}
	public Info(Integer infoId, String title, String type, String typeName,
			String infoStatus, String keyWord, String infoValidity,
			String detail, Date startDate, Date modifyDate, Date endDate,
			Date delDate, String ipAddreess, String area, Integer censor) {
		super();
		this.infoId = infoId;
		this.title = title;
		this.type = type;
		this.typeName = typeName;
		this.infoStatus = infoStatus;
		this.keyWord = keyWord;
		this.infoValidity = infoValidity;
		this.detail = detail;
		this.startDate = startDate;
		this.modifyDate = modifyDate;
		this.endDate = endDate;
		this.delDate = delDate;
		this.ipAddreess = ipAddreess;
		this.area = area;
		this.censor = censor;
	}
	public Integer getInfoId() {
		return infoId;
	}
	public void setInfoId(Integer infoId) {
		this.infoId = infoId;
	}
	public String getTitle() {
		return title;
	}
	public void setTitle(String title) {
		this.title = title;
	}
	public String getType() {
		return type;
	}
	public void setType(String type) {
		this.type = type;
	}
	public String getTypeName() {
		return typeName;
	}
	public void setTypeName(String typeName) {
		this.typeName = typeName;
	}
	public String getInfoStatus() {
		return infoStatus;
	}
	public void setInfoStatus(String infoStatus) {
		this.infoStatus = infoStatus;
	}
	public String getKeyWord() {
		return keyWord;
	}
	public void setKeyWord(String keyWord) {
		this.keyWord = keyWord;
	}
	public String getInfoValidity() {
		return infoValidity;
	}
	public void setInfoValidity(String infoValidity) {
		this.infoValidity = infoValidity;
	}
	public String getDetail() {
		return detail;
	}
	public void setDetail(String detail) {
		this.detail = detail;
	}
	public Date getStartDate() {
		return startDate;
	}
	public void setStartDate(Date startDate) {
		this.startDate = startDate;
	}
	public Date getModifyDate() {
		return modifyDate;
	}
	public void setModifyDate(Date modifyDate) {
		this.modifyDate = modifyDate;
	}
	public Date getEndDate() {
		return endDate;
	}
	public void setEndDate(Date endDate) {
		this.endDate = endDate;
	}
	public Date getDelDate() {
		return delDate;
	}
	public void setDelDate(Date delDate) {
		this.delDate = delDate;
	}
	public String getIpAddreess() {
		return ipAddreess;
	}
	public void setIpAddreess(String ipAddreess) {
		this.ipAddreess = ipAddreess;
	}
	public String getArea() {
		return area;
	}
	public void setArea(String area) {
		this.area = area;
	}
	public Integer getCensor() {
		return censor;
	}
	public void setCensor(Integer censor) {
		this.censor = censor;
	}
	
	
	
	
}
