package com.bornsoft.asn.bean.quotation;

import java.io.Serializable;
import java.util.Date;

public class QuotationInfoStat implements Serializable{

    /**
	 * 
	 */
	private static final long	serialVersionUID	= -4167621586621524926L;
	private Integer statCode;
    private QuotationProduct quotationProduct;
    private QuotationMarket quotationMarket;
    private Float avgPrice;
    private String comments;
    private Date statDate;
    private Integer quotationCode;

    public QuotationMarket getQuotationMarket() {
        return quotationMarket;
    }

    public void setQuotationMarket(QuotationMarket quotationMarket) {
        this.quotationMarket = quotationMarket;
    }

    public QuotationProduct getQuotationProduct() {
        return quotationProduct;
    }

    public void setQuotationProduct(QuotationProduct quotationProduct) {
        this.quotationProduct = quotationProduct;
    }

    public Float getAvgPrice() {
        return avgPrice;
    }

    public void setAvgPrice(Float avgPrice) {
        this.avgPrice = avgPrice;
    }

    public Integer getQuotationCode() {
        return quotationCode;
    }

    public void setQuotationCode(Integer quotationCode) {
        this.quotationCode = quotationCode;
    }

    public Integer getStatCode() {
        return statCode;
    }

    public void setStatCode(Integer statCode) {
        this.statCode = statCode;
    }

    public String getComments() {
        return comments;
    }

    public void setComments(String comments) {
        this.comments = comments;
    }

    public Date getStatDate() {
        return statDate;
    }

    public void setStatDate(Date statDate) {
        this.statDate = statDate;
    }
}
